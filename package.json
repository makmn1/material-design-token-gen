{
  "name": "@makmn1/material-design-token-gen",
  "version": "0.1.0",
  "description": "Generate Material-style tokens (color, elevation, motion, shape, typography) as a plain object.",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "bin": {
    "scaffold-material-tokens": "./dist/bin/scaffold-material-tokens.js",
    "create-material-theme-tokens": "./dist/bin/create-material-theme-tokens.js"
  },
  "peerDependencies": {
    "@materialx/material-color-utilities": "^0.4.8"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@materialx/material-color-utilities": "^0.4.8",
    "@types/node": "^24.9.2",
    "@vitest/ui": "^4.0.8",
    "rimraf": "^6.0.1",
    "semantic-release": "^25.0.1",
    "typescript": "^5.6.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^4.0.6"
  },
  "license": "Apache-2.0",
  "keywords": [
    "material",
    "tokens",
    "typography",
    "color",
    "design-tokens"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/makmn1/material-design-token-gen"
  },
  "publishConfig": {
    "access": "public"
  }
}
